{
	"name": "@tanega/sveltekit-package-tailwind-template",
	"description": "Template to build Svelte component library with SvelteKit package.",
	"keywords": [
		"svelte",
		"svelte3",
		"svelte-components",
		"sveltejs",
		"template",
		"tailwind",
		"storybook",
		"package"
	],
	"bugs": {
		"url": "https://github.com/tanega/sveltekit-package-tailwind-template/issues"
	},
	"license": "MIT",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"pwtest": "playwright test",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"vitest": "vitest run src",
		"vitest:watch": "vitest src",
		"test": "jest src",
		"test:watch": "npm run test -- --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.19.0",
		"@playwright/test": "^1.25.0",
		"@storybook/addon-actions": "^6.5.10",
		"@storybook/addon-essentials": "^6.5.10",
		"@storybook/addon-interactions": "^6.5.10",
		"@storybook/addon-links": "^6.5.10",
		"@storybook/addon-svelte-csf": "^2.0.7",
		"@storybook/builder-vite": "^0.2.2",
		"@storybook/svelte": "^6.5.10",
		"@storybook/testing-library": "^0.0.13",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "next",
		"@sveltejs/vite-plugin-svelte": "^1.0.5",
		"@testing-library/dom": "^8.17.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.1",
		"@testing-library/user-event": "^14.4.3",
		"autoprefixer": "^10.4.9",
		"babel-jest": "^29.0.3",
		"babel-loader": "^8.2.5",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-storybook": "^0.6.4",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^29.0.3",
		"jest-environment-jsdom": "^29.0.3",
		"jsdom": "^20.0.0",
		"postcss": "^8.4.16",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.7.1",
		"svelte-jester": "^2.3.2",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0",
		"vitest": "^0.23.2",
		"vitest-svelte-kit": "^0.0.7"
	},
	"setupFilesAfterEnv": [
		"@testing-library/jest-dom/extend-expect"
	],
	"type": "module"
}
